			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.{js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"devDependencies": {
		"husky": "^4.2.5",
		"jest": "^26.0.1",
		"lint-staged": "^10.2.6",
		"prettier": "*",
		"rimraf": "^3.0.2",
		"rollup": "^2.10.8",
		"rollup-jest": "^0.0.2",
		"standard-version": "^8.0.0"
	},
	"dependencies": {}
}
