		"**/*.{js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"devDependencies": {
		"git-scripts": "^0.2.1",
		"jest": "^24.1.0",
		"lint-staged": "^8.1.5",
		"prettier": "^1.16.4",
		"rimraf": "^2.6.3",
		"rollup": "^1.4.0",
		"rollup-jest": "0.0.2",
		"standard-version": "^5.0.1"
	},
	"dependencies": {}
