	"description": "return a flat array of key-values from a deep nested structure",
	"author": "mylesj <hi@myles.ninja>",
	"license": "ISC",
	"repository": "github:mylesj/deep-entries",
	"bugs": "https://github.com/mylesj/deep-entries/issues",
	"keywords": [
		"object",
		"entries",
		"object.entries",
		"deep",
		"nested",
