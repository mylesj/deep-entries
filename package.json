{
	"name": "deep-entries",
	"version": "4.0.1",
	"description": "wrangle deep nested object entries",
	"author": "mylesj <hi@myles.ninja>",
	"license": "ISC",
	"repository": "github:mylesj/deep-entries",
	"bugs": "https://github.com/mylesj/deep-entries/issues",
	"keywords": [
		"object",
		"entries",
		"object.entries",
		"iterator",
		"generator",
		"nested",
		"array",
		"flatten",
		"flat",
		"deep",
		"deepentries",
		"deepentry",
		"wrangling",
		"deno"
	],
	"files": [
		"dist/",
		"deno.js",
		"types.d.ts"
	],
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"types": "types.d.ts",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "rollup -c",
		"test": "jest -c jest.config.json",
		"test-deno": "deno test ./test/_deno.js",
		"prerelease": "npm run build && npm run test && npm run test-deno",
		"release": "standard-version --commit-all"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.{ts,js,json,md}": [
			"prettier --write",
			"git add"
		]
	},
	"devDependencies": {
		"husky": "^4.2.5",
		"jest": "^26.0.1",
		"lint-staged": "^10.2.6",
		"prettier": "*",
		"rimraf": "^3.0.2",
		"rollup": "^2.10.8",
		"rollup-jest": "^0.0.2",
		"standard-version": "^8.0.0"
	},
	"dependencies": {}
}
