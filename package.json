{
	"name": "deep-entries",
	"version": "1.0.0",
	"description": "flatten nested object entries",
	"author": "mylesj <hi@myles.ninja>",
	"license": "ISC",
	"repository": "github:mylesj/deep-entries",
	"bugs": "https://github.com/mylesj/deep-entries/issues",
	"keywords": [
		"object",
		"entries",
		"object.entries",
		"iterator",
		"generator",
		"nested",
		"array",
		"flatten",
		"flat",
		"deep",
		"deepentries",
		"deepentry"
	],
	"files": [
		"dist/"
	],
	"main": "dist/index.cjs.js",
	"module": "dist/index.esm.js",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "rollup -c",
